ganhoPorHora = float(input("Digite quanto você ganha por hora: \n"))
horasTrabalhadas = int(input("Digite quantas horas você trabalhou no mês: \n"))

ganhoTotal = ganhoPorHora * horasTrabalhadas
print(f"* Salário bruto: R${ganhoTotal}")

custoInss = ganhoTotal * 0.08
print(f"* Inss: R${custoInss}")

custoSindicato = ganhoTotal * 0.05
print(f"* Sindicato: R${custoSindicato}")

salarioLiquido = ganhoTotal - custoInss - custoSindicato
print(f"* Salário Liquido: R${salarioLiquido}")
